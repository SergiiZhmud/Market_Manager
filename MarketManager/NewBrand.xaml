<Window x:Class="MarketManager.NewBrand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarketManager"
        mc:Ignorable="d"
        Title="NewBrand" Height="173.832" Width="166.355" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox Name="txtAddBrand" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="140" />
        <Button Name="btnAddBrand" Content="Add Brand" HorizontalAlignment="Left" Margin="43,80,0,0" VerticalAlignment="Top" Width="75" Click="btnAddBrand_Click"/>
        <Label Content="Enter new Brand Name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="140" HorizontalContentAlignment="Center"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="43,115,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>

    </Grid>
</Window>
